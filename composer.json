{
    "name": "jdperc/deep-copy-fixed",
    "type": "library",
    "description": "A fork of the great myclabs/DeepCopy bundle for Symfony",
    "keywords": ["clone", "copy", "duplicate", "object", "object graph"],
    "homepage": "https://github.com/jdperc/deep-copy-fixed",
    "license": "MIT",
    "autoload": {
        "psr-4": { "DeepCopy\\": "src/DeepCopy/" }
    },
